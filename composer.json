{
    "name": "ardiansyahirwan/filament-role-permission",
    "description": "custom filament role permission base on spatie and filament",
    "require": {
        "php": "^8.0 | ^8.1",
        "spatie/laravel-permission": "6.9.0",
        "filament/filament": "v3.2.97",
        "illuminate/support": "^11.19",
        "livewire/livewire": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "orchestra/testbench": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Ardiansyahirwan\\FilamentRolePermission\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "src\\CustomPermissionServiceProvider\\"
            ],
            "aliases": []
        }
    },
    "authors": [
        {
            "name": "ardiansyahirwan",
            "email": "ardiansyahirwan923@gmail.com"
        }
    ]
}